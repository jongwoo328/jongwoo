"languageserver": {
  "python": {
    "venvPath": "~/venv",
    "command": "python",
    "args": ["-m", "jedi-language-server"],
    "filetypes": ["python"]
  },
  "typescript": {
    "command": "tsserver",
    "args": ["--stdio"],
    "initializationOptions": {
	"autoImportCompletions": true
    },
    "filetypes": ["javascript", "typescript"]
  }
}

